<h2>Add Membership Type</h2>
<%= erb('membershiptypes/membershiptypes_nav'.to_sym)  %>

<form class="edit-item-form" action="/membershiptypes" method="post">

  <label for="membershiptype-name">Name</label>
  <input id="membershiptype-name" type="text" name="name" required>

  <label for="membershiptype-description">Description</label>
  <input type="text" name="description" required>

  <div id="form-btns">
    <input id="submit-btn" class="form-btn submit" type="submit" name="submit" value="Add New Membership Type">
  </div>

</form>
