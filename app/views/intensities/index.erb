<h2>All Intensities</h2>
<%= erb('intensities/intensities_nav'.to_sym)  %>

<table class='item-table'>
<thead>
  <tr class="item-table-sort-row">
    <th><a href="/intensities/sort/asc/id" class="item-table-sort">&uarr;</><a href="/intensities/sort/desc/id" class="item-table-sort">&darr;</></th>
    <th><a href="/intensities/sort/asc/name" class="item-table-sort">&uarr;</><a href="/intensities/sort/desc/name" class="item-table-sort">&darr;</></th>
  </tr>
<tr class="item-table-labels">
  <th>ID</th>
  <th>Name</th>
</tr>
</thead>
<tbody>
<% @intensities.each do |intensity| %>
<tr>
  <td class="item-id"><a href="/intensities/<%= intensity.id %>/edit"><%= intensity.id %></a></td>
  <td class="item-name"><%= intensity.name %></td>
</tr>
<% end %>
</tbody>
</table>
<p class="table-instructions">Click ID number to edit or delete<p>
