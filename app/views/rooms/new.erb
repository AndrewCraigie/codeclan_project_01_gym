<h2>Add Room</h2>
<%= erb('rooms/rooms_nav'.to_sym)  %>


<form class="edit-item-form" action="/rooms" method="post">

  <label for="room-name">Name</label>
  <input id="room-name" type="text" name="name" required>

  <div id="form-btns">
    <input id="submit-btn" class="form-btn submit" type="submit" name="submit" value="Add Room">
  </div>
</form>
